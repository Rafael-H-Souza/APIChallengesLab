{
  "name": "api-logistica-lab",
  "version": "1.0.0",
  "description": "API Log√≠stica - Lab",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test:unit": "jest --selectProjects unit --runInBand",
    "test:int": "jest --selectProjects integration --runInBand",
    "coverage": "jest --coverage --runInBand",
    "docker:build": "docker-compose -f docker-compose.test.yml build --no-cache",
    "docker:up": "npm run docker:build && docker-compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker:down": "docker-compose -f docker-compose.test.yml down -v",
    "build": "tsc -p tsconfig.json && npm run copy:swagger",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest --passWithNoTests && jest --runInBand",
    "test:ci": "jest --coverage --runInBand",
    "lint": "eslint . --ext .ts",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "copy:swagger": "copyfiles -u 1 src/swagger/swagger.json dist/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "multer": "^2.0.2",
    "sequelize": "^6.37.7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.3",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "copyfiles": "^2.4.1",
    "jest": "^30.0.5",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
